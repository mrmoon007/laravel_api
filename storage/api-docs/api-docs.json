                "tags": [
                    "Admin"
                ],
                "summary": "Get list of projects",
                "description": "Returns list of projects",
                "operationId": "getUserl",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
